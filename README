# Pull docker image

docker pull tensorflow/tensorflow:2.1.0-gpu-py3

# Build Code

docker build -t wavenet/latest .

# Run code
sudo docker run -v ~/WaveNet-Implementation/saved_data:/saved_data --gpus all -it --rm wavenet/latest
sudo docker run -v ~/WaveNet-Implementation/saved_data/:/saved_data --gpus all -it --rm --name wavenetbox wavenet/latest

docker run --gpus all -it --rm wavenet/latest

